{
  "name": "full-modal",
  "author": "mengen.dai",
  "module": "dist/full-modal.esm.js",
  "version": "1.0.6",
  "keywords": [
    "modal",
    "full-modal",
    "modal-modal",
    "full-draggable-modal"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/full-modal.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/full-modal.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@size-limit/preset-small-lib": "^11.1.2",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@types/react-resizable": "^3.0.7",
    "antd": "^5.13.3",
    "husky": "^9.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-resizable": "^3.0.5",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.70.0",
    "size-limit": "^11.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  }
}
